                <ui:VisualElement class="gray" style="flex-direction: row; align-items: center; min-width: 62px;">
                    <ui:Button display-tooltip-when-elided="true" name="PassButton" text=" " class="pass-button gray-bezel" style="flex-shrink: 1; margin-left: 0; margin-right: 5px; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; align-items: center; justify-content: center; border-top-left-radius: 0; border-bottom-left-radius: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; flex-direction: column; width: 24px; height: 24px;">
                        <ui:VisualElement class="gray-gray-light-bezel" style="flex-grow: 1; width: 100%; align-items: center; justify-content: center; height: auto;">
                            <ui:VisualElement name="PassIcon" style="width: 100%; height: auto; background-image: url(&apos;project://database/Assets/Images/email%20good.png?fileID=2800000&amp;guid=2c728686c15d181509e3d0dfd89f8ef2&amp;type=3#email good&apos;); flex-grow: 1;" />
                        </ui:VisualElement>
                    </ui:Button>
                    <ui:Button display-tooltip-when-elided="true" name="TrashButton" class="gray-bezel" style="flex-shrink: 1; margin-left: 0; margin-right: 5px; margin-top: 0; margin-bottom: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; align-items: center; justify-content: center; border-top-left-radius: 0; border-bottom-left-radius: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; width: 24px; height: 24px;">
                        <ui:VisualElement class="gray-gray-light-bezel" style="flex-grow: 1; align-items: center; justify-content: center; width: 100%;">
                            <ui:VisualElement name="TrashIcon" style="background-image: url(&apos;project://database/Assets/Images/delete%20email.png?fileID=2800000&amp;guid=3dbea668cffcfa74db92b65f3577d89b&amp;type=3#delete email&apos;); flex-grow: 1; width: 100%;" />
                        </ui:VisualElement>
