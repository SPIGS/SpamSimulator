                    </ui:VisualElement>
                    <ui:VisualElement name="SubjectContainer" style="width: 100%; flex-direction: row; flex-grow: 1;">
                        <ui:Label text="Subject:" display-tooltip-when-elided="true" name="SubjectTitle" class="font-scale" style="font-size: 24px;" />
