    <ui:Button display-tooltip-when-elided="true" name="MainItemButton" class="gray-gray-bezel" style="font-size: initial; border-top-left-radius: 0; border-bottom-left-radius: 0; border-top-right-radius: 0; border-bottom-right-radius: 0; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0; flex-grow: 1;">
        <ui:VisualElement class="gray-light-bezel" style="flex-grow: 1;">
            <ui:VisualElement style="flex-direction: row; flex-grow: 1;">
                <ui:VisualElement name="InboxItemInfoContainer" style="flex-grow: 1; align-items: flex-start; justify-content: flex-start;">
                    <ui:VisualElement name="FromContainer" style="width: 100%; flex-direction: row; flex-grow: 1;">
                        <ui:Label text="From:" display-tooltip-when-elided="true" name="FromTitle" class="font-scale" style="font-size: 24px;" />
